# expects four values, numerator, and zero (denominator) on stack
.error_division_by_zero:
    push ' '
    push ':'
    push 'o'
    push 'r'
    push 'e'
    push 'z'
    push ' '
    push 'y'
    push 'b'
    push ' '
    push 'n'
    push 'o'
    push 'i'
    push 's'
    push 'i'
    push 'v'
    push 'i'
    push 'd'
    call .print_error
